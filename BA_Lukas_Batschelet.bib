@article{font-casasecaMarginsGeographicalInformation2024,
  title = {From the Margins of {{Geographical Information Systems}}: {{Limitations}}, Challenges, and Proposals},
  shorttitle = {From the Margins of {{Geographical Information Systems}}},
  author = {Font-Casaseca, Nuria and Rodó-Zárate, Maria},
  date = {2024-08-01},
  journaltitle = {Progress in Human Geography},
  volume = {48},
  number = {4},
  pages = {421--436},
  publisher = {SAGE Publications Ltd},
  issn = {0309-1325},
  doi = {10.1177/03091325241240231},
  abstract = {Some of the most exciting progress to address central limitations in GIS is currently originating from the margins of cartographic traditions. This article explores the potential of a proactive engagement with mapping technologies from peripheral positions, such as humanist, feminist, decolonial, queer, and black perspectives, to overcome what we identify as five intrinsic challenges of GIS: the representation of place; emotions; scales; time and change; and relational approaches. The proposals deal with specific concerns that do not fit in existing GISystems and suggest how a creative engagement with mapping technologies further expands our understanding of what GIS could be.},
  langid = {english},
  note = {\textbf{From the Margins of GIS: Limitations, Challenges, and Proposals}\\
\mkbibemph{Font-Casaseca \& Rodó-Zárate (2024), Progress in Human Geography}
\par
\subsection{\textbf{Zentrale Idee}}

\par
GIS ist \textbf{nicht neutral}, sondern beruht auf bestimmten \textbf{epistemologischen und politischen Annahmen}. Der Artikel zeigt, wie \textbf{feministische, dekoloniale, queere und humanistische Perspektiven} von den Rändern der GIS-Diskurse aus zentrale \textbf{Limitierungen von GIS} kritisch aufarbeiten – und kreative Alternativen anbieten.
\par
\subsection{\textbf{Fünf zentrale Herausforderungen (core challenges of GIS)}}

\begin{enumerate}

\item \textbf{Place}: Komplexe Bedeutung von Ort (Platz vs. Raum)
\item \textbf{Emotions}: Repräsentation von Gefühlen
\item \textbf{Scale}: Körperliche / intime Skalen
\item \textbf{Time \& Change}: Zeitlichkeit und Wandel
\item \textbf{Relationality}: Relationale, nicht-lineare Verbindungen

\end{enumerate}

\subsection{\textbf{1. Place – Ort als gelebte Erfahrung}}

\begin{itemize}

\item GIS reduziert „Ort“ auf \textbf{objektive, kartesische Koordinaten}.
\item Lived place = Beziehungen, Emotionen, Erinnerungen, Identität → \textbf{nicht quantifizierbar}.
\item „Platial Information Systems“ als Alternative: Fokus auf Bedeutung, nicht nur Geometrie.
\item \textbf{Beispiel:} „I Was There“ (Holocaust-Map) – kombiniert subjektive Erinnerungen mit visueller Kodierung (Farben, Formen).
\item Feministische \& indigene Kartografie fordert \textbf{Dekolonisierung des Raumbegriffs} → Mapping jenseits kartografischer Sprache.

\end{itemize}

\subsection{\textbf{2. Emotions – Emotionale Dimensionen in Karten}}

\begin{itemize}

\item Emotionen sind zentral für \textbf{Raumerleben}, aber in GIS kaum darstellbar.
\item 
\par
Mapping als \textbf{emotionales Erkenntnisinstrument}:
\par
\begin{itemize}

\item Teilnahme-Methoden: Sketch Maps, Storymaps, digitale Archive.
\item Kartografische Gestaltung (Design) kann Emotionen erzeugen (Fish, 2021).

\end{itemize}

\item \textbf{Beispiel:} HarassMap (Ägypten) – Crowdsourcing sexueller Belästigung, Sichtbarmachung stiller Gewalt.
\item Gefordert: \textbf{Kombination narrativer, qualitativer Daten mit GIS}, um subalterne Stimmen einzubinden.

\end{itemize}

\subsection{\textbf{3. Scale – Intime Räume \& Körper als Maßstab}}

\begin{itemize}

\item GIS bevorzugt \textbf{standardisierte Maßstäbe} (z.\,B. administrative Grenzen).
\item 
\par
Viele soziale Prozesse finden auf \textbf{anderen Skalen} statt:
\par
\begin{itemize}

\item Zuhause, Körper, Pflege, Alltag.

\end{itemize}

\item \textbf{Feministische Geographie} macht diese Skalen sichtbar.
\item 
\par
Zwei Zielrichtungen:
\par
\begin{enumerate}

\item \textbf{„Bring back the bodies“} – Kritik an entkörperlichter Objektivität (Haraway, D’Ignazio \& Klein).
\item 
\par
\textbf{Cuerpo-Territorio \& Body Mapping}:
\par
\begin{itemize}

\item Körper als Raum sozialer Gewalt \textbf{und} Widerstand.
\item Visuelle Methode aus dekolonial-feministischem Aktivismus (Lateinamerika).

\end{itemize}

\end{enumerate}

\end{itemize}

\subsection{\textbf{4. Time \& Change – Zeitlichkeit erfassen}}

\begin{itemize}

\item GIS = tendenziell statisch. Schwierigkeit, \textbf{Veränderungen} oder \textbf{multiple Zeitlichkeiten} darzustellen.
\item Zeit ist \textbf{politisch}: z.\,B. Migration, Gentrifizierung, Kolonialismus.
\item \textbf{Narrative Karten} können Geschichten des Wandels erzählen.
\item \textbf{Beispiel:} Queering the Map – queere Erinnerungsorte aus Vergangenheit und Gegenwart → Kartierung als kollektives Gedächtnis.
\item \textbf{Million Dollar Blocks}: zeigt Kosten und Raum-Zeit-Dynamiken von Inhaftierung in US-Städten.

\end{itemize}

\subsection{\textbf{5. Relationality – Beziehungen sichtbar machen}}

\begin{itemize}

\item GIS betont räumliche Nähe, aber nicht die \textbf{sozialen und politischen Verflechtungen} über Distanzen hinweg.
\item \textbf{Relationale Konzepte von Raum} (Massey): Ort als Knotenpunkt von Prozessen.
\item 
\par
Innovative Ansätze:
\par
\begin{itemize}

\item „Enfolding“: Raum wird relational „gefaltet“ statt linear gemessen.
\item 
\par
\textbf{Relief Maps} (Rodó-Zárate): Orte durch Erfahrungen von Ungleichheit verbunden.
\par
\begin{itemize}

\item Dimensionen: Ort, Machtachsen, Emotionen.
\item Visualisierung von Komfort/Unbehagen → \textbf{topologische Repräsentation sozialer Raumbeziehungen}.

\end{itemize}

\end{itemize}

\end{itemize}

\subsection{\textbf{Zentrale Thesen \& Beiträge}}

\begin{itemize}

\item GIS ist \textbf{nicht objektiv}: es erzeugt Repräsentationen, die Machtverhältnisse (re)produzieren.
\item 
\par
GIS-Systeme ignorieren:
\par
\begin{itemize}

\item komplexe Bedeutungen von Ort,
\item Körperlichkeit,
\item affektive Erfahrungen,
\item relationale Prozesse.

\end{itemize}

\item 
\par
Die \textbf{„Margins“ (Ränder)} sind epistemologisch und methodisch produktiv:
\par
\begin{itemize}

\item Queere, feministische, indigene, schwarze Perspektiven.
\item Interdisziplinär: Design, Storytelling, Digital Humanities.

\end{itemize}

\end{itemize}

\subsection{\textbf{Fazit}}

\begin{itemize}

\item GIS muss \textbf{neu gedacht} werden – nicht nur technisch, sondern epistemologisch.
\item Nicht einfach: „mehr Stimmen hinzufügen“, sondern: \textbf{Technologie, Praxis \& Kultur hinterfragen}.
\item 
\par
Vorschlag: \textbf{Critical GIS Praxis}:
\par
\begin{itemize}

\item Unsettling technologies
\item Reimagining categories (Place, Scale, Time…)
\item \textbf{Positioniertes Wissen} (Haraway) statt universeller Objektivität

\end{itemize}

\item Ziel: GIS, das \textbf{inklusiv, relational, dynamisch} und \textbf{sozial gerecht} ist.

\end{itemize}},
  file = {/Users/luba/Zotero/storage/KL2IW3X2/Font-Casaseca and Rodó-Zárate - 2024 - From the margins of Geographical Information Syste.pdf}
}

