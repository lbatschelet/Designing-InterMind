@article{axelssonfiskChronicObstructivePulmonary2018,
  title = {Chronic {{Obstructive Pulmonary Disease}} in {{Sweden}}: {{An}} Intersectional Multilevel Analysis of Individual Heterogeneity and Discriminatory Accuracy},
  shorttitle = {Chronic {{Obstructive Pulmonary Disease}} in {{Sweden}}},
  author = {Axelsson Fisk, Sten and Mulinari, Shai and Wemrell, Maria and Leckie, George and Perez Vicente, Raquel and Merlo, Juan},
  date = {2018-04-01},
  journaltitle = {SSM - Population Health},
  shortjournal = {SSM - Population Health},
  volume = {4},
  pages = {334--346},
  issn = {2352-8273},
  doi = {10.1016/j.ssmph.2018.03.005},
  url = {https://www.sciencedirect.com/science/article/pii/S2352827317302033},
  urldate = {2025-07-21},
  abstract = {Socioeconomic, ethnic and gender disparities in Chronic Obstructive Pulmonary Disease (COPD) risk are well established but no studies have applied multilevel analysis of individual heterogeneity and discriminatory accuracy (MAIHDA) within an intersectional framework to study this outcome. We study individuals at the first level of analysis and combinations of multiple social and demographic categorizations (i.e., intersectional strata) at the second level of analysis. Here we used MAIHDA to assess to what extent individual differences in the propensity of developing COPD are at the intersectional strata level. We also used MAIHDA to determine the degree of similarity in COPD incidence of individuals in the same intersectional stratum. This leads to an improved understanding of risk heterogeneity and of the social dynamics driving socioeconomic and demographic disparities in COPD incidence. Using data from 2,445,501 residents in Sweden aged 45–65, we constructed 96 intersectional strata combining categories of age, gender, income, education, civil- and migration status. The incidences of COPD ranged from 0.02\% for young, native males with high income and high education who cohabited to 0.98\% for older native females with low income and low education who lived alone. We calculated the intra-class correlation coefficient (ICC) that informs on the discriminatory accuracy of the categorizations. In a model that conflated additive and interaction effects, the ICC was good (20.0\%). In contrast, in a model that measured only interaction effects, the ICC was poor (1.1\%) suggesting that most of the observed differences in COPD incidence across strata are due to the main effects of the categories used to construct the intersectional matrix while only a minor share of the differences are attributable to intersectional interactions. We found conclusive interaction effects. The intersectional MAIHDA approach offers improved information to guide public health policies in COPD prevention, and such policies should adopt an intersectional perspective.},
  keywords = {Equity in health,Incidence of Chronic Obstructive Pulmonary Disease,Individual heterogeneity,Intersectionality,Multilevel analysis,Respiratory epidemiology,Socioeconomic determinants of health},
  file = {/Users/luba/Zotero/storage/DWJSAZ98/Axelsson Fisk et al. - 2018 - Chronic Obstructive Pulmonary Disease in Sweden An intersectional multilevel analysis of individual.pdf;/Users/luba/Zotero/storage/VLRKHUPK/S2352827317302033.html}
}

@article{baackDataficationEmpowermentHow2015,
  title = {Datafication and Empowerment: {{How}} the Open Data Movement Re-Articulates Notions of Democracy, Participation, and Journalism},
  shorttitle = {Datafication and Empowerment},
  author = {Baack, Stefan},
  date = {2015-12-01},
  journaltitle = {Big Data \& Society},
  volume = {2},
  number = {2},
  publisher = {SAGE Publications Ltd},
  issn = {2053-9517},
  doi = {10.1177/2053951715594634},
  url = {https://doi.org/10.1177/2053951715594634},
  urldate = {2025-08-06},
  abstract = {This article shows how activists in the open data movement re-articulate notions of democracy, participation, and journalism by applying practices and values from open source culture to the creation and use of data. Focusing on the Open Knowledge Foundation Germany and drawing from a combination of interviews and content analysis, it argues that this process leads activists to develop new rationalities around datafication that can support the agency of datafied publics. Three modulations of open source are identified: First, by regarding data as a prerequisite for generating knowledge, activists transform the sharing of source code to include the sharing of raw data. Sharing raw data should break the interpretative monopoly of governments and would allow people to make their own interpretation of data about public issues. Second, activists connect this idea to an open and flexible form of representative democracy by applying the open source model of participation to political participation. Third, activists acknowledge that intermediaries are necessary to make raw data accessible to the public. This leads them to an interest in transforming journalism to become an intermediary in this sense. At the same time, they try to act as intermediaries themselves and develop civic technologies to put their ideas into practice. The article concludes with suggesting that the practices and ideas of open data activists are relevant because they illustrate the connection between datafication and open source culture and help to understand how datafication might support the agency of publics and actors outside big government and big business.},
  langid = {english},
  note = {Baack analysiert, wie das Open-Data-Movement zentrale Vorstellungen von Demokratie, Partizipation und Journalismus neu definiert. Er argumentiert, dass die Bewegung zwar den Anspruch hat, Empowerment und Transparenz durch offene Daten zu fördern, dabei aber technokratische und elitäre Logiken reproduziert.
\par
\textbf{Hauptargumente:}
\par
\begin{itemize}

\item \textbf{Re-Artikulation von Demokratie:}\\
Open Data wird als Mittel präsentiert, um bürgerliche Partizipation zu stärken. Doch anstelle einer substantiellen Demokratisierung betont die Bewegung oft ein datenbasiertes Verständnis von Rationalität und Evidenz, das demokratische Prozesse auf technisches Wissen und quantitative Logik reduziert.
\item \textbf{Technologischer Determinismus:}\\
Viele Diskurse im Open-Data-Kontext unterstellen, dass Transparenz und Empowerment automatisch aus Offenheit folgen. Baack kritisiert diese Vorstellung als deterministisch und zeigt auf, dass technisches Wissen und Zugang zu Infrastruktur ungleich verteilt sind.
\item \textbf{Wandel journalistischer Praktiken:}\\
Der Autor beschreibt, wie datenjournalistische Projekte zwar neue Formen investigativer Recherche ermöglichen, gleichzeitig aber hohe technische Einstiegshürden erzeugen, die nicht alle Akteur:innen überwinden können. Dies führt zu neuen Machtasymmetrien innerhalb des Journalismus.
\item \textbf{Partizipation und Expertise:}\\
Die Bewegung fördert ein Modell von Partizipation, das auf technischer Kompetenz basiert. Dies bevorzugt privilegierte Gruppen, insbesondere solche mit technischem oder wissenschaftlichem Hintergrund, und schliesst marginalisierte Stimmen strukturell aus.

\end{itemize}

\par
\textbf{Fazit:}\\
Obwohl die Open-Data-Bewegung emanzipatorische Ziele verfolgt, bleibt sie häufig in einem neoliberalen Technikverständnis gefangen. Baack ruft dazu auf, das Versprechen digitaler Partizipation kritisch zu reflektieren und stärker auf soziale Gerechtigkeit, Inklusion und machtkritische Perspektiven zu fokussieren.},
  file = {/Users/luba/Zotero/storage/UPG5ULG5/Baack - 2015 - Datafication and empowerment How the open data movement re-articulates notions of democracy, partic.pdf}
}

@incollection{baerRedoingFeminismDigital2017,
  title = {Redoing Feminism: Digital Activism, Body Politics, and Neoliberalism},
  shorttitle = {Redoing Feminism},
  booktitle = {Digital {{Feminisms}}},
  author = {Baer, Hester},
  date = {2017},
  publisher = {Routledge},
  abstract = {In May 2014, Twitter exploded with a new wave of posts under the hashtag \#YesAllWomen,  a campaign drawing attention to the ubiquity of sexism, misogyny, and violence against  women. Users posted individual stories of discrimination, harassment, and fear, underscoring  the fact that “yes, all women” are subject to sexual violence. In Germany, feminists contributed  to \#YesAllWomen with posts about sexual discrimination and violence that also included the  hashtag \#Aufschrei (outcry), creating a transnational digital connection between two locally  grounded protest actions, with reference to the Twitter campaign documenting women’s  experiences of everyday sexism in Germany that created widespread public resonance in  2013. For example, German feminist activist Anke Domscheit-Berg (@anked) wrote in a  Twitter posting on May 25, 2014, “\#Yesallwomen-the global \#outcry … Because awareness  is the fi rst step towards social change.” 1 \#YesAllWomen and \#Aufschrei demonstrate theABSTRACT   This article investigates the renewed feminist politics that emerge  from the interface of digital platforms and activism today,  examining the role of digital media in aff ecting the particular  ways that contemporary feminist protests make meaning and  are understood transnationally, nationally, and locally. I consider  the political investments of digital feminisms in the context of  what Angela McRobbie has termed the “undoing of feminism” in  neoliberal societies, where discourses of choice, empowerment, and  individualism have made feminism seem both second nature and  unnecessary. Within this context, I describe a range of recent feminist  protest actions that are in a sense redoing feminism for a neoliberal  age. A key component of this redoing is the way recent protest actions  play out central tensions within historical and contemporary feminist  discourse; crucial here is the interrelationship between body politics  experienced locally and feminist actions whose effi  cacy relies on their  translocal and transnational articulation. My discussion focuses on  three case studies: SlutWalk Berlin, Peaches’ “Free Pussy Riot!” video,  and the Twitter campaigns \#Aufschrei and \#YesAllWomen. My  analysis ultimately calls attention to the precarity of digital feminisms,  which refl ect both the oppressive nature of neoliberalism and the  possibilities it off ers for new subjectivities and social formations.},
  isbn = {978-1-315-40622-0},
  pagetotal = {18}
}

@article{bakolisUrbanMindUsing2018,
  title = {Urban {{Mind}}: {{Using Smartphone Technologies}} to {{Investigate}} the {{Impact}} of {{Nature}} on {{Mental Well-Being}} in {{Real Time}}},
  shorttitle = {Urban {{Mind}}},
  author = {Bakolis, Ioannis and Hammoud, Ryan and Smythe, Michael and Gibbons, Johanna and Davidson, Neil and Tognin, Stefania and Mechelli, Andrea},
  date = {2018-02-01},
  journaltitle = {BioScience},
  shortjournal = {BioScience},
  volume = {68},
  number = {2},
  pages = {134--145},
  issn = {0006-3568},
  doi = {10.1093/biosci/bix149},
  abstract = {Existing evidence on the beneficial effects of nature on mental health comes from studies using cross-sectional designs. We developed a smartphone-based tool (Urban Mind; www.urbanmind.info) to examine how exposure to natural features within the built environment affects mental well-being in real time. The tool was used to monitor 108 individuals who completed 3013 assessments over a 1-week period. Significant immediate and lagged associations with mental well-being were found for several natural features. These associations were stronger in people with higher trait impulsivity, a psychological measure of one's tendency to behave with little forethought or consideration of the consequences, which is indicative of a higher risk of developing mental-health issues. Our investigation suggests that the benefits of nature on mental well-being are time-lasting and interact with an individual's vulnerability to mental illness. These findings have potential implications from the perspectives of global mental health as well as urban planning and design.},
  note = {\textbf{Urban Mind: Using Smartphone Technologies to Investigate the Impact of Nature on Mental Well-Being in Real Time}\\
\mkbibemph{Bakolis et al. (2018), BioScience}
\par
\subsection{\textbf{Zentrale Idee}}

\par
Entwicklung und Einsatz der Smartphone-App \textbf{Urban Mind}, um den \textbf{Einfluss natürlicher Elemente} im urbanen Raum auf das \textbf{mentale Wohlbefinden} in Echtzeit zu untersuchen.
\par
\subsection{\textbf{Forschungsfragen \& Hypothesen}}

\begin{enumerate}

\item \textbf{Fördert} der Aufenthalt in der Natur \textbf{momentanes mentales Wohlbefinden}?
\item Gibt es \textbf{zeitverzögerte Effekte} (Lag-Effekte)?
\item Sind die Effekte \textbf{stärker bei impulsiven Personen}, die als besonders anfällig für psychische Erkrankungen gelten?

\end{enumerate}

\subsection{\textbf{Kontext \& Problemstellung}}

\begin{itemize}

\item 66\,\% der Weltbevölkerung werden bis 2050 in Städten leben.
\item Stadtbewohner*innen haben ein \textbf{höheres Risiko für psychische Erkrankungen} (z.\,B. Depression, Angst, Psychose).
\item 
\par
Frühere Studien:
\par
\begin{itemize}

\item meist \textbf{querschnittlich}, also keine zeitliche Dynamik.
\item keine Berücksichtigung individueller \textbf{Vulnerabilität}.
\item keine genaue Erfassung spezifischer Naturmerkmale.

\end{itemize}

\end{itemize}

\subsection{\textbf{Methodologie: Ecological Momentary Assessment (EMA) per App}}

\subsubsection{\textbf{Vorteile der EMA per Smartphone:}}

\begin{itemize}

\item Echtzeit-Daten in realer Umgebung → hohe \textbf{ökologische Validität}.
\item Mehrere Messungen täglich → Erfassung von \textbf{dynamischen Zuständen}.
\item Geringe Kosten, hohe Reichweite → \textbf{Citizen Science}.

\end{itemize}

\subsubsection{\textbf{Urban Mind App}}

\begin{itemize}

\item Entwicklung: gemeinsam mit Artists \& Engineers.
\item Plattformen: iOS \& Android.
\item 
\par
Inhalte:
\par
\begin{itemize}

\item \textbf{Basisdaten}: Demografie, Impulsivität, mentale Gesundheit.
\item \textbf{7 EMA pro Tag} (à 2 Minuten) für 7 Tage = max. 49 Einträge pro Person.
\item 
\par
Fragen zu:
\par
\begin{itemize}

\item Umgebung (z.\,B. Bäume, Himmel, Wasser, Vogelgesang, Naturkontakt)
\item mentalem Wohlbefinden (Momentaufnahme, adaptiert von WEMWBS)
\item GPS-Position

\end{itemize}

\item Optional: \textbf{Fotos} und \textbf{Tonaufnahmen} → Förderung der Teilnahme \& potenzielle Auswertung durch GIS / Geonarratives.

\end{itemize}

\end{itemize}

\subsection{\textbf{Stichprobe}}

\begin{itemize}

\item N = 108 Teilnehmende (Selbstselektion via Social Media \& Medien)
\item Durchschnittsalter: 31,1 Jahre
\item Über 90\,\% mit Universitätsabschluss
\item 3013 EMA-Einträge → Hauptanalysen basieren auf Subgruppe mit {$>$}50\,\% Antwortquote (N = 64)

\end{itemize}

\subsection{\textbf{Zentrale Ergebnisse}}

\subsubsection{1. \textbf{Unmittelbare Effekte}}

\begin{itemize}

\item 
\par
Signifikant höheres momentanes Wohlbefinden bei:
\par
\begin{itemize}

\item \textbf{Draußen sein}
\item \textbf{Bäume sehen}
\item \textbf{Himmel sehen}
\item \textbf{Vögel hören}
\item \textbf{sich mit Natur verbunden fühlen}

\end{itemize}

\item 
\par
Effektstärken (Δ Wohlbefinden):
\par
\begin{itemize}

\item z.\,B. Vogelgesang: +3.82 Punkte, Bäume sehen: +1.78 Punkte

\end{itemize}

\end{itemize}

\subsubsection{2. \textbf{Zeitverzögerte Effekte (Lagged Effects)}}

\begin{itemize}

\item 
\par
Auch \textbf{2–5 Stunden nach Naturkontakt} signifikante Effekte.
\par
\begin{itemize}

\item Beispiel: Bäume sehen → +1.62 Punkte im nächsten EMA (nach ∅ 2h25min)
\item Beispiel: Naturgefühl → +1.70 Punkte zwei EMAs später (∅ 4h50min)

\end{itemize}

\end{itemize}

\subsubsection{3. \textbf{Interaktion mit Impulsivität}}

\begin{itemize}

\item 
\par
Effekt von Natur \textbf{stärker bei Personen mit hoher Impulsivität}.
\par
\begin{itemize}

\item Signifikante Interaktionen bei: draußen sein, Bäume, Vogelgesang, Naturgefühl.

\end{itemize}

\end{itemize}

\subsection{\textbf{Stärken \& Beiträge der Studie}}

\begin{itemize}

\item \textbf{Innovatives Echtzeit-Design} (EMA via Smartphone).
\item Erfassung \textbf{konkreter Naturelemente} (nicht nur „Grünraum allgemein“).
\item Kombination aus \textbf{sozialwissenschaftlicher, psychologischer und stadtplanerischer Perspektive}.
\item 
\par
Beleg für:
\par
\begin{itemize}

