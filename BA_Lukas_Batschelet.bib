  title = {A Multilevel Approach to Modeling Health Inequalities at the Intersection of Multiple Social Identities},
  author = {Evans, Clare R. and Williams, David R. and Onnela, Jukka-Pekka and Subramanian, S. V.},
  date = {2018-04-01},
  journaltitle = {Social Science \& Medicine},
