@article{dignazioGeographiesMissingData2024,
  title = {Geographies of Missing Data: {{Spatializing}} Counterdata Production against Feminicide},
  shorttitle = {Geographies of Missing Data},
  author = {D’Ignazio, Catherine and Cruxên, Isadora and Martinez Cuba, Angeles and Suárez Val, Helena and Dogan, Amelia and Ansari, Natasha},
  date = {2024-09-05},
  journaltitle = {Environment and Planning D: Society and Space},
  shortjournal = {Environ Plan D},
  publisher = {SAGE Publications Ltd STM},
  issn = {0263-7758},
  doi = {10.1177/02637758241275961},
  url = {https://doi.org/10.1177/02637758241275961},
  urldate = {2024-10-14},
  abstract = {Feminicide is the gender-related killing of cisgender and transgender women and girls. It reflects patriarchal and racialized systems of oppression and reveals how territories and socio-economic landscapes configure everyday gender-related violence. In recent decades, many grassroots data production initiatives have emerged with the aim of monitoring this extreme but invisibilized phenomenon. We bridge scholarship in feminist and information geographies with data feminism to examine the ways in which space, broadly defined, shapes the counterdata production strategies of feminicide data activists. Drawing on a qualitative study of 33 monitoring efforts led by civil society organizations across 15 countries, primarily in Latin America, we provide a conceptual framework for examining the spatial dimensions of data activism. We show how there are striking transnational patterns related to where feminicide goes unrecorded, resulting in geographies of missing data. In response to these omissions, activists deploy multiple spatialized strategies to make these geographies visible, to situate and contextualize each case of feminicide, to reclaim databases as spaces for memory and witnessing, and to build transnational networks of solidarity. In this sense, we argue that data activism about feminicide constitutes a space of resistance and resignification of everyday forms of gender-related violence.},
  langid = {english},
  note = {Das Paper untersucht, wie fehlende Daten – also Datenlücken, Auslassungen oder systematische Leerstellen – nicht nur technische, sondern zutiefst politische und räumliche Phänomene darstellen. Die Autor*innen plädieren für eine geographisch informierte Analyse von sogenannten „Counterdata“-Bewegungen, also Initiativen, die bewusst alternative, häufig feministische oder dekoloniale Datensätze produzieren, um dominante Infrastrukturen zu hinterfragen.
\par
\textbf{Zentrale Argumente:}
\par
\begin{itemize}

\item Fehlende Daten sind nicht zufällig, sondern spiegeln Machtverhältnisse wider: Bestimmte Körper, Orte oder Erfahrungen werden systematisch nicht erfasst, weil sie als irrelevant gelten oder nicht in dominante Wissenssysteme passen.
\item Diese Leerstelle ist nicht nur ein Defizit, sondern ein Ausdruck epistemischer Gewalt – besonders gegenüber marginalisierten Gruppen.
\item Die Autor*innen schlagen vor, fehlende Daten als \mkbibemph{geographies of absence} zu begreifen, also als räumlich produzierte und verortete Ausdrucksformen sozialer Ausschlüsse.
\item Counterdata-Projekte – wie etwa feministische Mapping-Initiativen, queere Datenarchive oder indigene Kartierungsprojekte – versuchen, diese Lücken bewusst zu füllen. Sie produzieren nicht nur neue Daten, sondern stellen die Grundlagen von Datenproduktion, -infrastruktur und -ethik infrage.

\end{itemize}

\par
\textbf{Relevanz für kritische Geographie:}\\
Das Paper verbindet feministische epistemologische Perspektiven mit räumlicher Analyse und legt dar, wie alternative Datenpraktiken nicht nur methodisch, sondern auch politisch transformativ sein können. Es fordert eine Geographie der Datenlücken – nicht nur um Abwesenheiten zu identifizieren, sondern um zu zeigen, wo und wie Wissen systematisch unterdrückt oder unsichtbar gemacht wird.
\par
\textbf{Bezug zur eigenen Arbeit:}\\
Die Analyse der \mkbibemph{geographies of missing data} unterstreicht die Notwendigkeit einer bewussten, gerechteren Datenpraxis. Gerade bei der Entwicklung eigener Tools oder Befragungsinstrumente wird deutlich, dass schon die Frage, \mkbibemph{was} überhaupt erhoben wird, politisch ist. Das Paper liefert damit eine wichtige theoretische Grundlage für die Argumentation zugunsten einer feministischen, reflexiven Datenpraxis.},
  file = {/Users/luba/Zotero/storage/643RAYAG/D’Ignazio et al. - 2024 - Geographies of missing data Spatializing counterd.pdf}
}

