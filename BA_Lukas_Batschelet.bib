  title = {The Mental Health Benefits of Visiting Canals and Rivers: {{An}} Ecological Momentary Assessment Study},
  shorttitle = {The Mental Health Benefits of Visiting Canals and Rivers},
  author = {Bergou, Nicol and Hammoud, Ryan and Smythe, Michael and Gibbons, Jo and Davidson, Neil and Tognin, Stefania and Reeves, Graeme and Shepherd, Jenny and Mechelli, Andrea},
