@article{evansTutorialConductingIntersectional2024,
  title = {A Tutorial for Conducting Intersectional Multilevel Analysis of Individual Heterogeneity and Discriminatory Accuracy ({{MAIHDA}})},
  author = {Evans, Clare R. and Leckie, George and Subramanian, S. V. and Bell, Andrew and Merlo, Juan},
  date = {2024-06-01},
