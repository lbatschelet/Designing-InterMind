@article{elwoodFeministDigitalGeographies2018,
  title = {Feminist Digital Geographies},
  author = {Elwood, Sarah and Leszczynski, Agnieszka},
  date = {2018-05-04},
  journaltitle = {Gender, Place \& Culture},
  volume = {25},
  number = {5},
  pages = {629--644},
  publisher = {Routledge},
  issn = {0966-369X},
  doi = {10.1080/0966369X.2018.1465396},
  url = {https://doi.org/10.1080/0966369X.2018.1465396},
  urldate = {2025-08-06},
  abstract = {At a moment when disciplinary attentions are turning to the digital as a subject and object of geographic inquiry, we consider enduring contours and new directions in feminist digital geographies scholarship. We revisit the centrality of feminist critiques of Science to critical digital geographies and their predecessors, identifying axes of scholarly engagement that have emerged from feminist theory and praxis. Simultaneously, we acknowledge the resounding whiteness and heteronormativity of these theoretical origins. In the second half of the article, we trace new horizons of contemporary digital geographies scholarship that engage queer and critical race theory, postcolonial feminism, and black and queer code studies. These theoretical moves give voice to longstanding silences and are indispensable to a political and ethical digital geographic scholarship and praxis, as well as to re-making our technologies and ourselves as digital subjects.},
  keywords = {Black code studies,critical race geographies,digital geographies,feminist theory,queer theory},
  note = {Elwood und Leszczynski untersuchen, wie feministische Geographien digitale Technologien, Datenpraktiken und deren gesellschaftliche Effekte kritisch analysieren und gestalten können. Sie plädieren für eine feministische digitale Geographie, die nicht nur digitale Technologien untersucht, sondern diese auch als Mittel feministischer Praxis versteht.
\par
\subsubsection{\textbf{Zentrale Argumente und Konzepte}}

\begin{itemize}

\item \textbf{Digitale Technologien als Machtpraktiken}: Digitale Geographien müssen als politische Felder verstanden werden, in denen Macht, Kontrolle und Repräsentation ausgehandelt werden. Feministische Perspektiven helfen, diese Felder kritisch zu beleuchten.
\item \textbf{Materialität und Embodiment}: Im Gegensatz zu techno-utopischen Narrativen betonen Elwood \& Leszczynski die Körperlichkeit digitaler Praktiken. Digitale Technologien sind immer materiell eingebettet, etwa in Form von Geräten, Infrastrukturen oder Arbeitsbedingungen.
\item \textbf{Situated Knowledge und Partial Perspectives}: Aufbauend auf Donna Haraways Konzept des „situierten Wissens“ fordern die Autorinnen eine reflexive Haltung, die digitale Forschung nicht als neutral, sondern als politisch verortet begreift.
\item \textbf{Feministische Kritik an Plattformen und Datenpraktiken}: Die Autorinnen warnen vor der Reproduktion von Ungleichheiten durch datenbasierte Technologien (z.\,B. Algorithmen, Datensätze) und betonen, dass diese soziale Hierarchien nicht nur abbilden, sondern aktiv (re)produzieren.
\item \textbf{Digitale Praktiken als Orte feministischer Intervention}: Feministische digitale Geographie bedeutet nicht nur Analyse, sondern auch aktives Eingreifen. Beispiele sind partizipative Mapping-Projekte, kritische Datenvisualisierung oder Community-basierte Technologien.

\end{itemize}

\subsubsection{\textbf{Verhältnis zu bisherigen Debatten}}

\begin{itemize}

\item Elwood und Leszczynski positionieren sich gegen technikdeterministische oder unkritisch euphorische Perspektiven der digitalen Geographie.
\item Sie bauen auf feministisch-geographische Theorietraditionen (Haraway, Rose, Moss) auf, und übertragen deren zentrale Einsichten in den digitalen Raum.
\item Ihr Beitrag ist nicht nur theoretisch, sondern auch methodologisch relevant: Sie fordern eine Forschung, die inklusiv, kritisch und handlungsorientiert ist.

\end{itemize}

\subsubsection{\textbf{Relevanz für deine Arbeit}}

\par
Das Paper bietet eine starke theoretische Grundlage für eine kritische, feministische Auseinandersetzung mit digitalen Tools, Plattformen und Datenpraktiken. Besonders wichtig ist die Verbindung von feministischer Theorie mit Fragen der digitalen Souveränität, Sichtbarkeit und Gerechtigkeit – zentrale Themen in deiner Argumentation für Open Source, Partizipation und Machtkritik in der Softwareentwicklung.},
  file = {/Users/luba/Zotero/storage/D4C4XJSK/Elwood and Leszczynski - 2018 - Feminist digital geographies.pdf}
}

