  shorttitle = {The {{Healthy Environments}} and {{Active Living}} for {{Translational Health}} ({{HEALTH}}) {{Platform}}},
  author = {Wray, Alexander and O'Bright, Katelyn and Zhong, Shiran and Doherty, Sean and Luubert, Michael and Long, Jed and Reining, Catherine and Lemieux, Christopher and Salter, Jon and Gilliland, Jason},
