@article{hammoudLonelyCrowdInvestigating2021,
  title = {Lonely in a Crowd: Investigating the Association between Overcrowding and Loneliness Using Smartphone Technologies},
  shorttitle = {Lonely in a Crowd},
  author = {Hammoud, Ryan and Tognin, Stefania and Bakolis, Ioannis and Ivanova, Daniela and Fitzpatrick, Naomi and Burgess, Lucie and Smythe, Michael and Gibbons, Johanna and Davidson, Neil and Mechelli, Andrea},
  date = {2021-12-20},
  journaltitle = {Scientific Reports},
  shortjournal = {Sci Rep},
  volume = {11},
  number = {1},
  publisher = {Nature Publishing Group},
  issn = {2045-2322},
  doi = {10.1038/s41598-021-03398-2},
  url = {https://www.nature.com/articles/s41598-021-03398-2},
  urldate = {2025-07-15},
  abstract = {Loneliness is a major public health concern with links to social and environmental factors. Previous studies have typically investigated loneliness as a stable emotional state using retrospective cross-sectional designs. Yet people experience different levels of loneliness throughout the day depending on their surrounding environment. In the present study, we investigated the associations between loneliness and social and environmental factors (i.e. overcrowding, population density, social inclusivity and contact with nature) in real-time. Ecological momentary assessment data was collected from participants using the Urban Mind smartphone application. Data from 756 participants who completed 16,602 assessments between April 2018 and March 2020 were used in order to investigate associations between momentary feeling of loneliness, the social environment (i.e. overcrowding, social inclusivity, population density) and the built environment (i.e. contact with nature) using multilevel modelling. Increased overcrowding and population density were associated with higher levels of loneliness; in contrast, social inclusivity and contact with nature were associated with lower levels of loneliness. These associations remained significant after adjusting for age, gender, ethnicity, education and occupation. The positive association between social inclusivity and lower levels of loneliness was more pronounced when participants were in contact with nature, indicating an interaction between the social and built environment on loneliness. The feeling of loneliness changes in relation to both social and environmental factors. Our findings have potential implications for public health strategies and interventions aimed at reducing the burden of loneliness on society. Specific measures, which would increase social inclusion and contact with nature while reducing overcrowding, should be implemented, especially in densely populated cities.},
  langid = {english},
  keywords = {Epidemiology,Health policy,Human behaviour,Lifestyle modification,Psychology and behaviour},
  file = {/Users/luba/Zotero/storage/FGPH7HK9/Hammoud et al. - 2021 - Lonely in a crowd investigating the association between overcrowding and loneliness using smartphon.pdf}
}

@article{hammoudSmartphonebasedEcologicalMomentary2022,
  title = {Smartphone-Based Ecological Momentary Assessment Reveals Mental Health Benefits of Birdlife},
  author = {Hammoud, Ryan and Tognin, Stefania and Burgess, Lucie and Bergou, Nicol and Smythe, Michael and Gibbons, Johanna and Davidson, Neil and Afifi, Alia and Bakolis, Ioannis and Mechelli, Andrea},
  date = {2022-10-27},
  journaltitle = {Scientific Reports},
  shortjournal = {Sci Rep},
  volume = {12},
  number = {1},
  publisher = {Nature Publishing Group},
  issn = {2045-2322},
  doi = {10.1038/s41598-022-20207-6},
  url = {https://www.nature.com/articles/s41598-022-20207-6},
  urldate = {2025-07-15},
  abstract = {The mental health benefits of everyday encounters with birdlife for mental health are poorly understood. Previous studies have typically relied on retrospective questionnaires or artificial set-ups with little ecological validity. In the present study, we used the Urban Mind smartphone application to examine the impact of seeing or hearing birds on self-reported mental wellbeing in real-life contexts. A sample of 1292 participants completed a total of 26,856 ecological momentary assessments between April 2018 and October 2021. Everyday encounters with birdlife were associated with time-lasting improvements in mental wellbeing. These improvements were evident not only in healthy people but also in those with a diagnosis of depression, the most common mental illness across the world. These findings have potential implications for both environmental and wildlife protection and mental healthcare policies. Specific measures, aimed at preserving and increasing everyday encounters with birdlife in urban areas, should be implemented.},
  langid = {english},
  keywords = {Epidemiology,Health policy,Human behaviour,Lifestyle modification,Psychology},
  file = {/Users/luba/Zotero/storage/FB6V2LFB/Hammoud et al. - 2022 - Smartphone-based ecological momentary assessment reveals mental health benefits of birdlife.pdf}
}

@article{hammoudSmartphonebasedEcologicalMomentary2024,
  title = {Smartphone-Based Ecological Momentary Assessment Reveals an Incremental Association between Natural Diversity and Mental Wellbeing},
  author = {Hammoud, Ryan and Tognin, Stefania and Smythe, Michael and Gibbons, Johanna and Davidson, Neil and Bakolis, Ioannis and Mechelli, Andrea},
  date = {2024-04-16},
  journaltitle = {Scientific Reports},
  shortjournal = {Sci Rep},
  volume = {14},
  number = {1},
  publisher = {Nature Publishing Group},
  issn = {2045-2322},
  doi = {10.1038/s41598-024-55940-7},
  abstract = {Using smartphone-based ecological momentary assessment, this study investigated an association between natural diversity on mental wellbeing. A sample of 1,998 participants completed 41,448 assessments between April 2018 and September 2023. Environments which included a larger range of natural features, such as trees, plants and birdlife (high natural diversity) were associated with greater mental wellbeing than environments including a smaller range of natural features (low natural diversity). There was evidence of a mediating effect of natural diversity on the association between natural environments and mental wellbeing. These results highlight the importance of policies and practices that support richness of biodiversity for public mental health.},
  langid = {english},
  keywords = {Environmental impact,Environmental social sciences,measuring well-being,Psychology,Psychology and behaviour},
  note = {\textbf{Smartphone-Based Ecological Momentary Assessment Reveals an Incremental Association Between Natural Diversity and Mental Wellbeing}\\
\mkbibemph{Hammoud et al. (2024), Scientific Reports}
\par
\subsection{\textbf{Zentrale Idee}}

\par
Erstmals wird in Echtzeit und im Alltag untersucht, \textbf{wie sich die Vielfalt natürlicher Elemente (natural diversity)} auf das \textbf{momentane mentale Wohlbefinden} auswirkt – nicht nur das Vorhandensein von Natur, sondern \textbf{deren Vielfalt} wird als wichtiger Einflussfaktor betrachtet.
\par
\subsection{\textbf{Forschungsfragen \& Hypothesen}}

\begin{enumerate}

\item Wirken sich spezifische Naturmerkmale (z.\,B. Bäume, Vogelgesang, Wasser) \textbf{positiv und nachhaltig} auf das Wohlbefinden aus?
\item Führt eine höhere \textbf{Vielfalt} dieser Merkmale zu \textbf{größeren mentalen Gesundheitsvorteilen}?
\item Vermittelt die \textbf{Vielfalt} den Zusammenhang zwischen Naturumgebung und Wohlbefinden?

\end{enumerate}

\subsection{\textbf{Kontext}}

\begin{itemize}

\item 70\,\% der Weltbevölkerung werden bis 2050 in Städten leben.
\item Stadtleben erhöht Risiko für psychische Krankheiten – ein möglicher Grund: \textbf{fehlende Naturerfahrungen}.
\item 
\par
Bisherige Forschung konzentrierte sich oft auf:
\par
\begin{itemize}

\item Quantität (z.\,B. Nähe zu Parks),
\item Dauer des Aufenthalts,
\item oder visuelle Reize in Laborsettings – \textbf{wenig zu Vielfalt in realen Alltagsräumen}.

\end{itemize}

\end{itemize}

\subsection{\textbf{Theoretische Fundierung}}

\begin{itemize}

\item \textbf{Attention Restoration Theory (ART)}: Natur hilft, erschöpfte kognitive Ressourcen wiederherzustellen.
\item \textbf{Stress Recovery Theory (SRT)}: Natur reduziert physiologische Stressreaktionen.

\end{itemize}

\subsection{\textbf{Methodologie: Ecological Momentary Assessment (EMA) via App}}

\subsubsection{\textbf{Urban Mind App}}

\begin{itemize}

\item Kooperation: King’s College London, J\&L Gibbons, Nomad Projects.
\item \textbf{Teilnahme:} weltweit, 2018–2023, n = 1998 (Hauptsample).
\item \textbf{Dauer:} 14 Tage, 3 EMA/Tag = 42 Messzeitpunkte.
\item 
\par
Fragen zu:
\par
\begin{itemize}

\item mentalem Wohlbefinden (10 Items: 5 positiv, 5 negativ),
\item sicht- oder hörbare Natur (Bäume, Pflanzen, Wasser, Vogelgesang),
\item GPS-Standort,
\item Foto + 8 Sekunden Audio (zur Beteiligung, nicht für Analyse genutzt).

\end{itemize}

\end{itemize}

\subsubsection{\textbf{Natural Diversity Score}}

\begin{itemize}

\item 
\par
0–4 Punkte je EMA:
\par
\begin{itemize}

\item 1 Punkt pro beobachteter Naturkomponente: Baum, Pflanze, Vogel, Wasser.

\end{itemize}

\end{itemize}

\subsection{\textbf{Ergebnisse}}

\subsubsection{1. \textbf{Unmittelbare Effekte}}

\begin{itemize}

\item 
\par
Alle Naturmerkmale positiv assoziiert mit höherem Wohlbefinden (p {$<$} 0.001).
\par
\begin{itemize}

\item \textbf{Vögel}: +1.56 Punkte
\item \textbf{Pflanzen}: +1.31 Punkte
\item \textbf{Bäume}: +1.06 Punkte
\item \textbf{Wasser}: +1.60 Punkte

\end{itemize}

\end{itemize}

\subsubsection{2. \textbf{Zeitverzögerte Effekte}}

\begin{itemize}

\item \textbf{Vögel, Pflanzen, Bäume}: auch nach ca. 8\,h signifikant positiv.
\item \textbf{Wasser}: kein signifikanter Lag-Effekt.

\end{itemize}

\subsubsection{3. \textbf{Natural Diversity}}

\begin{itemize}

\item 
\par
Linearer Zusammenhang: \textbf{mehr Vielfalt = mehr Wohlbefinden}.
\par
\begin{itemize}

\item z.\,B. bei 50\,\%-Sample: +0.74 Punkte pro zusätzlichem Naturmerkmal.
\item Effekt bleibt auch nach 8\,h bestehen (+0.20 Punkte).

\end{itemize}

\end{itemize}

\subsubsection{4. \textbf{Mediationsanalyse}}

\begin{itemize}

\item 
\par
\textbf{23.4\,\% des Effekts} von „in Natur sein“ auf Wohlbefinden wird über Vielfalt vermittelt.
\par
\begin{itemize}

\item Total Effect: +3.89 Punkte
\item Direct Effect: +2.98 Punkte
\item Indirect (via diversity): +0.91 Punkte

\end{itemize}

\end{itemize}

\subsection{\textbf{Stärken}}

\begin{itemize}

\item \textbf{Realwelt-Daten} in Echtzeit, hohe ökologische Validität.
\item \textbf{Längsschnittdesign}: Erfassung dynamischer Tagesverläufe.
\item Große Samplegröße, differenzierte Analysen (25\,\%/50\,\%/75\,\% Completion Rates).
\item \textbf{Berücksichtigung soziodemografischer Confounder} (Alter, Geschlecht, Bildung etc.).

\end{itemize}

\subsection{\textbf{Limitationen}}

\begin{itemize}

\item \textbf{Selbstselektion}: evtl. Bias durch naturaffine Personen.
\item \textbf{Nicht-repräsentative Stichprobe} (jung, gut gebildet, Smartphone-affin).
\item \textbf{Keine Kausalität}: beobachtendes Design.
\item \textbf{Subjektive Maße}: z.\,B. Erfassung der Naturmerkmale durch Laien (Gefahr der Überschätzung).
\item \textbf{Kein Einbezug weiterer Umweltfaktoren} (z.\,B. Lärm, Wetter, städtischer Kontext).

\end{itemize}

\subsection{\textbf{Diskussion \& Bedeutung}}

\begin{itemize}

\item \textbf{Vielfalt} zählt: Nicht nur grün ≠ gut, sondern \textbf{bunt = besser}.
\item Effekt auch \textbf{zeitverzögert spürbar} (bis zu 8\,h).
\item 
\par
Unterstützung für \textbf{naturbasierte Interventionen}:
\par
\begin{itemize}

\item Urbanes Design → vielfältige, sensorisch reiche Räume.
\item Weg von Monokulturen (Rasenflächen) hin zu \textbf{polykulturellen Biodiversitätsinseln}.

\end{itemize}

\item Relevanz für \textbf{Public Health}, \textbf{Stadtplanung} \& \textbf{Umwelterziehung}.

\end{itemize}

\subsection{\textbf{Fazit}}

\begin{itemize}

\item Diese Studie zeigt: \textbf{Mehr natürliche Vielfalt in Städten erhöht nachweislich das momentane Wohlbefinden}.
\item Vielfalt wirkt dabei nicht nur im Moment, sondern \textbf{über Stunden hinweg}.
\item Urbanes Naturdesign muss sich \textbf{nicht nur an Fläche, sondern an Diversität} orientieren.
\item App-basierte Forschung ermöglicht \textbf{partizipative, globale und skalierbare Ansätze} für zukünftige Studien.

