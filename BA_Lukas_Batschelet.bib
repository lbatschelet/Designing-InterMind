@article{randallDevelopmentTrialMobile2013,
  title = {Development and {{Trial}} of a {{Mobile Experience Sampling Method}} (m-{{ESM}}) for {{Personal Music Listening}}},
  author = {Randall, William M. and Rickard, Nikki S.},
  date = {2013-12-01},
  journaltitle = {Music Perception},
  shortjournal = {Music Perception},
  volume = {31},
  number = {2},
  pages = {157--170},
  publisher = {University of California Press},
  issn = {0730-7829},
  doi = {10.1525/mp.2013.31.2.157},
  url = {https://online.ucpress.edu/mp/article/31/2/157/62583/Development-and-Trial-of-a-Mobile-Experience},
  urldate = {2025-07-25},
  abstract = {The measurement of everyday music use remains a challenge for researchers, with many of the available methodologies limited by intrusiveness or lack of ecological validity. The Experience Sampling Method (ESM) addresses such limitations by assessing current subjective experience at various times throughout participants’ everyday functioning. The aim of the current project was to develop and trial a mobile ESM (m-ESM) capable of collecting event-related data during natural listening episodes. This methodology was designed to maintain a natural and familiar listening experience for participants, and to collect real-time data on personal music listening. An application (app) was created which utilized mobile-device technology, and allowed combination of experience sampling with a personal music player. Analyses were performed on trial data from 101 participants to determine the efficacy of the m-ESM. Results indicated that this methodology would maintain ecological validity and cause minimal intrusion into everyday activities of the listener. Questionnaires were answered immediately at the time of listening, minimizing the problem of retrospective recall biases. This innovative methodology allows for the collection of a wealth of listening data that will advance the accurate measurement of everyday, personal music listening.},
  langid = {english}
}

@article{rodo-de-zarateDevelopingGeographiesIntersectionality2014,
  title = {Developing Geographies of Intersectionality with {{Relief Maps}}: Reflections from Youth Research in {{Manresa}}, {{Catalonia}}},
  shorttitle = {Developing Geographies of Intersectionality with {{Relief Maps}}},
  author = {Rodó-de-Zárate, Maria},
  date = {2014-09-14},
  journaltitle = {Gender, Place \& Culture},
  volume = {21},
  number = {8},
  pages = {925--944},
  publisher = {Routledge},
  issn = {0966-369X},
  doi = {10.1080/0966369X.2013.817974},
  abstract = {The conceptualisation of intersectionality has been one of the most important contributions to feminism, as it allows for theorisation about multiple and intersecting oppressions. This contributes to a more complex and dynamic understanding of social relations and power structures, and it acknowledges the differences between categories, but it has no methodological direction. I try to take this debate a step forward, by developing what I have called Relief Maps: a new way of collecting, analysing and displaying intersectional data. I consider Relief Maps to be a sound tool for studying the Geographies of Intersectionality, as they show the relationship between three dimensions: power structures (the social), lived experience (the psychological) and places (the geographical). By showing some examples of them, I demonstrate how Relief Maps make empirical work on intersectionality possible and how they are able to take into account both privilege and oppression without using categories in a fixed and rigid way. Taking the spatial dimension as a central part of the analysis, they show how the relationship between power structures varies depending on places and also illustrate how subject formations are done and undone through everyday spaces. Relief Maps aim to take the potentialities of intersectionality and minimise its limitations: they intend to disrupt homogeneous categories while pointing towards the material consequences of oppression. Finally, Relief Maps also provide some insights on intersectionality itself, as they help to think about how power structures relate to each other and the role that experience and place play in these processes.},
  keywords = {experiencia vivida,feminism,feminismo,interseccionalidad,Intersectionality,lived experience,lugar,Mapas de Relieves de la Experiencia,place,Relief Maps,地形图,地方,多元交织性,女性主义,生活经验},
  note = {\textbf{Developing Geographies of Intersectionality with Relief Maps}\\
\mkbibemph{Maria Rodó-de-Zárate (2014),} \mkbibemph{Gender, Place \& Culture}
\par
\subsection{\textbf{Zentrale Idee}}

\par
Rodó-de-Zárate entwickelt \textbf{Relief Maps} als Methode zur \textbf{empirischen Analyse von Intersektionalität} – mit Fokus auf den Zusammenhang von \textbf{Machtstrukturen, gelebter Erfahrung und Raum}.
\par
\subsection{\textbf{Hintergrund \& Kritik an Intersektionalität}}

\begin{itemize}

\item Ursprünge: Crenshaw (1991), Hooks, Collins – Kritik an einheitlicher „Frauen“-Kategorie.
\item Wichtig: Machtstrukturen wie \textbf{Gender, Race, Class, Sexuality, Age} wirken \textbf{gleichzeitig} und \textbf{verändern sich im Raum}.
\item 
\par
Kritik:
\par
\begin{itemize}

\item Begriffsunschärfe (vage, unvollständig).
\item Keine klare \textbf{methodologische Richtung}.
\item Gefahr der \textbf{Essenzialisierung} (wenn Kategorien als fix betrachtet werden).
\item Risiko des „Oppression Olympics“ (wer ist am meisten unterdrückt?).

\end{itemize}

\end{itemize}

\subsection{\textbf{Relief Maps – Konzeption}}

\subsubsection{\textbf{Ziel:}}

\begin{itemize}

\item Sichtbarmachen von Intersektionalität \textbf{ohne feste Kategorien}.
\item Analyse von \textbf{Erfahrungen in Bezug auf Orte und Machtverhältnisse}.

\end{itemize}

\subsubsection{\textbf{Drei Dimensionen:}}

\begin{enumerate}

\item \textbf{Soziale Dimension}: Machtstrukturen (Gender, Sexualität, etc.)
\item \textbf{Geografische Dimension}: Orte (z.\,B. Schule, Straße, Zuhause)
\item \textbf{Psychologische Dimension}: Gelebte Erfahrung (Komfort ↔ Unbehagen)

\end{enumerate}

\subsubsection{\textbf{Begriff "Relief"}:}

\begin{itemize}

\item Topografisch: Höhen und Tiefen = Erfahrungen von Privileg / Unterdrückung.
\item Emotional: Orte der \textbf{Entlastung} vs. Orte der \textbf{Belastung}.

\end{itemize}

\subsection{\textbf{Konstruktion der Relief Maps}}

\begin{itemize}

\item Grundlage: Interviews + Fokusgruppen.
\item Ziel: Erfahrungsräume junger Menschen in Manresa (Spanien) verstehen.
\item 
\par
Vorgehen:
\par
\begin{enumerate}

\item Orte aus Alltag der Befragten identifizieren.
\item Erfahrungen zu jedem Ort in Bezug auf Identitäten erfassen.
\item Punkte (für jede Machtstruktur) auf einer Skala von Unbehagen zu Komfort platzieren.
\item Linien verbinden die Punkte → zeigen Bewegung, Entwicklung, Ausweichstrategien.
\item Pfeile zeigen \textbf{intensivierende oder abschwächende Wechselwirkungen} zwischen Machtstrukturen.

\end{enumerate}

\end{itemize}

\subsection{\textbf{Klassifikation von Orten (nicht starr):}}

\begin{itemize}

\item \textbf{Orte der Unterdrückung}
\item \textbf{Kontroverse Orte} (Ambivalenz)
\item \textbf{Neutrale Orte}
\item \textbf{Orte der Entlastung} (z.\,B. feministische Gruppenräume, Zuhause)

\end{itemize}

\subsection{\textbf{Beispiele}}

\subsubsection{\textbf{Queralt (17, lesbisch, weiß):}}

\begin{itemize}

\item Fußballplatz = ambivalent: Empowerment durch queeres Umfeld, aber Ausschluss wegen Klasse.
\item Les Carpes (Nachtclub) = starke Unterdrückung (Gewalt wegen Homosexualität).
\item Ateneu \& Zuhause = Orte der Entlastung.
\item Pfeile zeigen z.\,B. wie \textbf{Genderstrategien} (Kapuzenpulli) helfen, sich im öffentlichen Raum sicherer zu fühlen.

\end{itemize}

\subsubsection{\textbf{Laila (26, marokkanisch, heterosexuell):}}

\begin{itemize}

\item Zuhause und Nachbarschaft = starke Kontrolle durch Religion \& Ehemann → \textbf{Genderunterdrückung}.
\item Barcelona = Ort der Entlastung.
\item Ethnizität spielt ambivalente Rolle: wird nicht direkt als unterdrückend erlebt, aber intensiviert Genderkontrolle.

\end{itemize}

\subsubsection{\textbf{Nil (21, weiß, cis, hetero):}}

\begin{itemize}

\item Meistens neutrale Erfahrungen → \textbf{sichtbar wird das Privileg}.
\item Nur Zuhause: Alterskonflikt.
\item Relief Map zeigt \textbf{Normalität der Privilegierung}.

\end{itemize}

\subsection{\textbf{Potenziale \& Weiterentwicklungen}}

\begin{itemize}

\item \textbf{Zeitdimension} integrierbar (z.\,B. Dauer an einem Ort → Farbabstufung).
\item \textbf{Körper} als Ort (Ort der Unterdrückung / Entlastung).
\item \textbf{Größe der Punkte} = Grad der Identifikation mit Ort.
\item Verknüpfung mit \textbf{GIS}: Verortung der Relief Maps in realen Karten (vgl. Kwan 2002).
\item Verwendung in \textbf{therapeutischen / pädagogischen Kontexten}.

\end{itemize}

\subsection{\textbf{Beitrag zur Theoriebildung}}

\begin{itemize}

\item 
\par
Relief Maps:
\par
\begin{itemize}

\item Vermeiden \textbf{fixe Kategorien}, zeigen \textbf{strukturelle Positionierungen}.
\item \textbf{Keine additive Logik} (nicht einfach: Frau + queer = doppelt unterdrückt).
\item Zeigen \textbf{Verhältnisse \& Dynamiken} zwischen Machtachsen.
\item Kombinieren \textbf{materialistische Perspektive} (Raum, Körper) mit \textbf{poststrukturalistischer Kritik} (Kategorien hinterfragen).

\end{itemize}

\end{itemize}

\subsection{\textbf{Fazit}}

\begin{itemize}

\item \textbf{Raum ist zentral} für das Erleben von Machtverhältnissen.
\item 
\par
Relief Maps ermöglichen:
\par
\begin{itemize}

\item eine \textbf{situierte Analyse von Intersektionalität},
\item \textbf{vergleichende Darstellungen} ohne Vereinfachung,
\item neue Formen von \textbf{Empirie \& Visualisierung} in der feministischen Geographie.

\end{itemize}

\item Sie bieten eine \textbf{praktische Methodologie} und \textbf{konzeptionelles Werkzeug} zugleich.

\end{itemize}},
  file = {/Users/luba/Zotero/storage/T5952LYY/Rodó-de-Zárate - 2014 - Developing geographies of intersectionality with R.pdf}
}

