	journal = {Gender, Place \& Culture},
	author = {Piedalue, Amy},
	month = apr,
	year = {2017},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/0966369X.2016.1219323},
	keywords = {Domestic violence, Islamophobia, islamofobia, patriarcados regionales, plural resistance, regional patriarchies, resistencia plural, structural violence, violencia doméstica, violencia estructural, 伊斯兰恐惧症, 区域復权体制, 多重抵抗。, 家庭暴力, 结构性暴力},
